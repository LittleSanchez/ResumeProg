<Page x:Class="ResumeProg.Views.Pages.StartPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResumeProg.Views.Pages"
        mc:Ignorable="d" Background="White"
        Title="StartWindow" Height="450" Width="800" x:Name="window">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.ColumnSpan="2">
            <Label x:Name="path" Content="" Margin="40" FontSize="16" HorizontalContentAlignment="Center"/>
            <Button Content="Save To" Width="200" Height="30"/>
        </StackPanel>
    </Grid>
</Page>
